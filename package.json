{
  "name": "prisma-dbml-generator",
  "version": "0.1.0",
  "description": "Prisma DBML Generator",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "dev": "tsc -p . && npm run prisma:generate",
    "prisma:save": "npx prisma migrate save --experimental",
    "prisma:up": "npx prisma migrate up --experimental",
    "prisma:generate": "npx prisma generate",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "changlog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "keywords": [
    "prisma",
    "dbml"
  ],
  "contributors": [
    {
      "name": "Marc Stammerjohann"
    },
    {
      "name": "Gary Gro√ügarten"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@dbml/core": "~2.1.1",
    "@prisma/client": "~2.5.0",
    "@prisma/generator-helper": "~2.5.0"
  },
  "devDependencies": {
    "@prisma/cli": "~2.5.0",
    "@types/node": "~14.6.0",
    "prettier": "~2.0.5",
    "typescript": "~3.9.7"
  }
}
